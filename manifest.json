{
    "name":"Whitko Kiosk",
    "version":"0.1.0.0",
    "manifest_version":2,
    "minimum_chrome_version":"37.0.0.0",
    "default_locale":"en_US",
    "permissions":
        [
            "webview",
            "power",
            "storage",
            "videoCapture",
            "geolocation",
            "pointerLock",
            "system.display",
            {"fileSystem":["write","retainEntries","directory"]},
            "accessibilityFeatures.read",
            "accessibilityFeatures.modify"
        ],
    "app":
        {
            "background":{"scripts":["js/foam.js","js/cab.js","config.js","background_main.js"]}},
            "kiosk_enabled":true,
            "icons":{"128":"img/128.png"}
        }
